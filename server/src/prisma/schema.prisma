generator client {
  provider = "prisma-client-js"
}

generator docs {
  provider = "node node_modules/prisma-docs-generator"
  output   = "../../dbDocs"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model onlinelist {
  id       Int      @id @default(autoincrement())
  userId   String
  nickname String
  status   String
  date     DateTime
}

model messages {
  id       Int      @id @default(autoincrement())
  userId   String
  message  String?
  nickname String
  date     DateTime
}

model logs {
  id      Int    @id @default(autoincrement())
  message String
  time    String
}

model errors {
  id      Int     @id @default(autoincrement())
  message String?
  time    String
}
