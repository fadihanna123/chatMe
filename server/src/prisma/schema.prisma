generator client {
  provider = "prisma-client-js"
}

generator docs {
  provider = "node node_modules/prisma-docs-generator"
  output   = "../../dbDocs"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model logs {
  id      Int     @id @default(autoincrement())
  method  String
  message String?
  located String
  time    String
}

model errors {
  id      Int     @id @default(autoincrement())
  errorId Int
  method  String
  message String?
  located String
  time    String
}
